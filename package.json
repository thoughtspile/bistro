{
  "name": "bistro",
  "version": "0.0.2",
  "description": "Lightning-fast replacement for lodash.get",
  "main": "dist/index.js",
  "scripts": {
    "test": "node test/unit",
    "test:perf": "mkdir -p .tmp && node --expose-gc test/perf > .tmp/perf.json",
    "bundle": "babel src --out-dir lib --presets=env && webpack",
    "prepublishOnly": "npm run bundle"
  },
  "keywords": [
    "lodash",
    "utility",
    "util",
    "access",
    "get"
  ],
  "author": "v.klepov",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "benchmark": "^2.1.4",
    "fast-get": "^1.0.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "object-path": "^0.11.4",
    "randomstring": "^1.1.5",
    "webpack": "^3.11.0"
  }
}
